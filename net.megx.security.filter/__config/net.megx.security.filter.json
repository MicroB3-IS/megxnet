{
	path: '$resources/net.megx.security.filter',
	filter:{
		enabled: true,
		entrypoints:[
			{
				name: 'standardLogin',
				class: 'net.megx.security.filter.impl.WebLoginSecurity',
				urlPattern: '/(apps|services/|protected/|oauth/authorize|admin|j_security_check).*',
				order: 1
			},
			{
				name: 'oauth_v1_security',
				class: 'net.megx.security.filter.impl.OAuth_1_Security',
				urlPattern: '(/oauth/(request_token|access_token)|/services/(res|pubmap)).*',
				order: 0
			}
		],
		managers:[
			{
				name: 'default'
			}
		]
	}
}
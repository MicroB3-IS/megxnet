{
	path: '/home/pavle/projects/iw/mb3/microb3/svn-workspace/megx.net/net.megx.security.filter',
	filter:{
		enabled: true,
		entrypoints:[
			{
				name: 'standardLogin',
				class: 'net.megx.security.filter.impl.WebLoginSecurity',
				urlPattern: '/(apps|services/|protected/|admin|j_security_check).*',
				order: 1
			},
			{
				name: 'oauth_v1_security',
				class: 'net.megx.security.filter.impl.OAuth_1_Security',
				urlPattern: '(/oauth/(request_token|access_token|authorize)|/(services|ws)/(res|pubmap)).*',
				order: 0
			},
			{
				name: 'annonymus',
				class: 'net.megx.security.filter.impl.AnonnimusAuthenticationEndpoint',
				urlPattern: '/.*',
				order: 2,
				enabled: false
			}
		],
		managers:[
			{
				name: 'default'
			}
		]
	}
}
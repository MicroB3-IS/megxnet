<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.megx.megdb.ws.data.mappers.WSDataMapper">
    <update id="executeDynamicUpdateStatement" parameterType="map">
        update ${schemaName}.${tableName}
        set ${fieldsToUpdate}
        where ${whereStatement}
    </update>
</mapper>
